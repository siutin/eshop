{
  "name": "Shop",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string"
    },
    "logo": {
      "type": "buffer"
    },
    "staffId": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "owner": {
      "type": "belongsTo",
      "model": "ShopStaff",
      "foreignKey": "staffId"
    },
    "shopMembers": {
      "type": "hasMany",
      "model": "ShopMember",
      "foreignKey": "shopId"
    }
  },
  "acls": [],
  "methods": {}
}
